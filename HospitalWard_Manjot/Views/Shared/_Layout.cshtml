<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Project Name</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/ProjectName/ProjectName/assets/materialize/css/materialize.min.css" media="screen,projection" />
    <!-- Bootstrap Styles-->
    <link href="~/ProjectName/ProjectName/assets/css/bootstrap.css" rel="stylesheet" />
    <!-- FontAwesome Styles-->
    <link href="~/ProjectName/ProjectName/assets/css/font-awesome.css" rel="stylesheet" />
    <!-- Morris Chart Styles-->
    <link href="~/ProjectName/ProjectName/assets/js/morris/morris-0.4.3.min.css" rel="stylesheet" />
    <!-- Custom Styles-->
    <link href="~/ProjectName/ProjectName/assets/css/custom-styles.css" rel="stylesheet" />
    <!-- Google Fonts-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />
    <link rel="stylesheet" href="~/ProjectName/ProjectName/assets/js/Lightweight-Chart/cssCharts.css">
</head>
<body>
    <div id="wrapper">
        <nav class="navbar navbar-default top-navbar" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle waves-effect waves-dark" data-toggle="collapse" data-target=".sidebar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand waves-effect waves-dark" href="index.html"><i class="large material-icons">track_changes</i> <strong>Hospital Ward</strong></a>
                <div id="sideNav" href=""><i class="material-icons dp48">toc</i></div>
            </div>
            <ul class="nav navbar-top-links navbar-right">
                <li><a class="dropdown-button waves-effect waves-dark" href="#!" data-activates="dropdown2"><i class="fa fa-bell fa-fw"></i> <i class="material-icons right">arrow_drop_down</i></a></li>
                <li><a class="dropdown-button waves-effect waves-dark" href="#!" data-activates="dropdown1"><i class="fa fa-user fa-fw"></i> <b>Admin</b> <i class="material-icons right">arrow_drop_down</i></a></li>
            </ul>
        </nav>
        <!-- Dropdown Structure -->
        <ul id="dropdown1" class="dropdown-content">
            <li>
                <a href="#"><i class="fa fa-user fa-fw"></i> My Profile</a>
            </li>
            <li>
                <a href="#"><i class="fa fa-gear fa-fw"></i> Settings</a>
            </li>
            <li>
                <a href="#"><i class="fa fa-sign-out fa-fw"></i> <partial name="_LoginPartial" /></a>
            </li>
        </ul>
        <ul id="dropdown2" class="dropdown-content w250">
            <li>
                <a>
                    <div>
                        <i class="fa fa-comment fa-fw"></i> New Comment
                        <span class="pull-right text-muted small">4 min</span>
                    </div>
                </a>
            </li>
            <li class="divider"></li>
            <li>
                <a>
                    <div>
                        <i class="fa fa-twitter fa-fw"></i> 3 New Followers
                        <span class="pull-right text-muted small">12 min</span>
                    </div>
                </a>
            </li>
            <li class="divider"></li>
            <li>
                <div>
                    <i class="fa fa-envelope fa-fw"></i> Message Sent
                    <span class="pull-right text-muted small">4 min</span>
                </div>
                </a>
            </li>
            <li class="divider"></li>
            <li>
                <a>
                    <div>
                        <i class="fa fa-tasks fa-fw"></i> New Task
                        <span class="pull-right text-muted small">4 min</span>
                    </div>
                </a>
            </li>
            <li class="divider"></li>
            <li>
                <a>
                    <div>
                        <i class="fa fa-upload fa-fw"></i> Server Rebooted
                        <span class="pull-right text-muted small">4 min</span>
                    </div>
                </a>
            </li>
            <li class="divider"></li>
            <li>
                <a class="text-center" href="#">
                    <strong>See All Alerts</strong>
                    <i class="fa fa-angle-right"></i>
                </a>
            </li>
        </ul>
        <!--/. NAV TOP  -->
        <nav class="navbar-default navbar-side" role="navigation">
            <div class="sidebar-collapse">
                <ul class="nav" id="main-menu">
                    <li>
                        <a class="active-menu waves-effect waves-dark" asp-area="" asp-controller="Home" asp-action="Index"><i class="fa fa-dashboard"></i>HOME</a>
                    </li>
                    <li>
                        <a class="waves-effect waves-dark" asp-area="" asp-controller="Doctors" asp-action="Index"><i class="fa fa-desktop"></i>DOCTORS</a>
                    </li>
                    <li>
                        <a class="waves-effect waves-dark" asp-area="" asp-controller="Nurses" asp-action="Index"><i class="fa fa-bar-chart-o"></i>NURSES</a>
                    </li>
                    <li>
                        <a class="waves-effect waves-dark" asp-area="" asp-controller="Patients" asp-action="Index"><i class="fa fa-qrcode"></i>PATIENTS</a>
                    </li>
                    <li>
                        <a class="waves-effect waves-dark" asp-area="" asp-controller="Wards" asp-action="Index"><i class="fa fa-table"></i>WARDS</a>
                    </li>
                    @*<li>
                            <a href="menu6.html" class="waves-effect waves-dark"><i class="fa fa-edit"></i> MENU 6</a>
                        </li>
                        <li>
                            <a href="menu6.html" class="waves-effect waves-dark"><i class="fa fa-sitemap"></i> MENU 7<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="#">sub menu </a>
                                </li>
                                <li>
                                    <a href="#">sub menu<span class="fa arrow"></span></a>
                                    <ul class="nav nav-third-level">
                                        <li>
                                            <a href="#">sub menu</a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </li>*@
                </ul>
            </div>
        </nav>
        <!-- /. NAV SIDE  -->
        <div id="page-wrapper">
            <div id="page-inner">

                @RenderBody()

                <footer>
                    <p>All right reserved <a href="#">Footer</a></p>
                </footer>
            </div>
            <!-- /. PAGE INNER  -->
        </div>
        <!-- /. PAGE WRAPPER  -->
    </div>
    <!-- /. WRAPPER  -->
    <!-- JS Scripts-->
    <!-- jQuery Js -->
    <script src="~/ProjectName/ProjectName/assets/js/jquery-1.10.2.js"></script>
    <!-- Bootstrap Js -->
    <script src="~/ProjectName/ProjectName/assets/js/bootstrap.min.js"></script>
    <script src="~/ProjectName/ProjectName/assets/materialize/js/materialize.min.js"></script>
    <!-- Metis Menu Js -->
    <script src="~/ProjectName/ProjectName/assets/js/jquery.metisMenu.js"></script>
    <!-- Morris Chart Js -->
    <script src="~/ProjectName/ProjectName/assets/js/morris/raphael-2.1.0.min.js"></script>
    <script src="~/ProjectName/ProjectName/assets/js/morris/morris.js"></script>
    <script src="~/ProjectName/ProjectName/assets/js/easypiechart.js"></script>
    <script src="~/ProjectName/ProjectName/assets/js/easypiechart-data.js"></script>
    <script src="~/ProjectName/ProjectName/assets/js/Lightweight-Chart/jquery.chart.js"></script>
    <!-- Custom Js -->
    <script src="~/ProjectName/ProjectName/assets/js/custom-scripts.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>


















